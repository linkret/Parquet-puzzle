{
  "name": "parquet-opt",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.cjs",
    "build": "vite build",
    "dev": "npm run build && nodemon server.cjs --debug",
    "dev:emul": "npm run firestore:seed && npm run dev",
    "firestore:emul": "firebase emulators:start --only firestore --project parquet-puzzle",
    "firestore:seed": "firebase emulators:exec --only firestore --project parquet-puzzle \"node db/seed-firestore.cjs\"",
    "firestore:emul:score": "set \"FIRESTORE_EMULATOR_HOST=127.0.0.1:8088\" && node db/print-scoreboard.cjs "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google-cloud/firestore": "^7.11.6",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "module-alias": "^2.2.3",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "nodemon": "^3.1.10",
    "seedrandom": "^3.0.5",
    "vite": "^5.0.0"
  },
  "type": "module",
  "_moduleAliases": {
    "@db": "db"
  }
}
